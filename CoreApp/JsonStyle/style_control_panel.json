{
    "QMainWindow": [
        {
            "tittle":"Media Cuts Studio",
            "frameless": true,
			"transluscentBg": true,
			"icon":":/web_icons/icons/web_icons/rectangle-3370.png"
            
        },
        {
			"navigation":[
				{
					"moveWindow": "centralwidget"
				},
				{
                    "minimize":"minimize_window_button",
					"close": "close_window_button",
					"restore":[
						{	
							"buttonName": "restore_window_button",
							"normalIcon": ":/feather/icons/feather/maximize-2.png",
							"maximizedIcon": ":/feather/icons/feather/minimize-2.png"
						}
					]
				}

			]
		}
    ],

    "QCustomQStackedWidget":[
		{
			"name":"stackedWidget",

			"transitionAnimation":[
				{
					"slide":[
						{
							"active":true,
							"duration": 1000,
							"direction": "horizontal",
							"easingCurve": "OutBack"
						}
					]
				}
			]
		},
		{
			"name":"stackedWidget_Main",
            "navigation":[
				{
					"navigationButtons":[
						{
							"Mode_Shortify": "page_shortify",
							"Mode_Batch_processing": "page_Batch_processing",
							"Mode_1_long_video": "page_1_long_video"
						}
					]
				}
            ],
			"transitionAnimation":[
				{
					
					"slide":[
						{
							"active":true,
							"duration": 1000,
							"direction": "horizontal",
							"easingCurve": "OutBack"
						}
					]
				}
			]
		}

	],
    "QCustomSlideMenu": [
        {
            "name": "widget_slidemenu",
            "defaultSize": [{
                "width": 0,
                "height": 755
            }],
            "collapsedSize": [{
                "width": 0,
                "height": 755
            }],
            "expandedSize": [{
                "width": 160,
                "height": "parent"
            }],

            "toggleButton": [{
                "buttonName": "pushButton_menu",
                "icons": [{
                    "whenMenuIsCollapsed": ":/material_design/icons/material_design/menu_open.png",
                    "whenMenuIsExpanded": ":/font_awesome/icons/font_awesome/lines-leaning.png"
                }]

            }]
        }

    ],
    "QSettings": [
        {
            "ThemeSettings": [
                {
                    "CustomTheme": [
                        {
                            "Background-color": "#ffffff",
                            "Text-color": "#000000",
                            "Accent-color": "#0F9696",
                            "Theme-name": "Light-Blue",
                            "Default-Theme": true,
                            "Icons-color": "#0F6464"
                        }
                    ]
                }
            ],
            "AppSettings": {
                "OrginizationName": "SoftwareAICompany",
                "ApplicationName": "Media Cuts Studio",
                "OrginizationDormain": "https://softwareai-company.com"
            }
        }
    ]

}
